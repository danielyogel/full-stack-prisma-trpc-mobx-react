{
  "name": "full-stack-server",
  "version": "0.0.1",
  "engines": {
    "node": ">= 16 <17"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@trpc/server": "9.25.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "fp-ts": "^2.16.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.3",
    "nanoid": "^4.0.2",
    "pluralize": "^8.0.0",
    "prisma": "^4.15.0",
    "qs": "^6.11.2",
    "superjson": "^1.12.3",
    "ts-toolbelt": "^9.6.0",
    "tsd": "^0.28.1",
    "tslib": "^2.5.3",
    "type-fest": "^3.12.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/pluralize": "^0.0.29",
    "@types/qs": "6.9.7",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsutils": "3.21",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "dev": "nodemon -e ts -w ./src -x NODE_ENV=development ts-node --pretty ./src/server.ts",
    "start": "NODE_ENV=development ts-node --pretty ./src/server.ts",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "typecheck": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "generate-prisma": "npx prisma generate",
    "format-prisma": "npx prisma format",
    "push-prisma": "npx prisma db push",
    "pull-prisma": "npx prisma db pull",
    "studio-prisma": "npx prisma studio",
    "install-production": "npm ci",
    "install-dev": "npm install && npm run generate-prisma"
  },
