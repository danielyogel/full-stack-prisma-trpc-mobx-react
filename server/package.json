{
  "name": "full-stack-server",
  "version": "0.0.1",
  "engines": {
    "node": ">= 16 <21"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@trpc/server": "^10.43.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "fp-ts": "^2.16.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.3",
    "nanoid": "^5.0.3",
    "pluralize": "^8.0.0",
    "prisma": "^5.5.2",
    "qs": "^6.11.2",
    "superjson": "^1.12.3",
    "ts-toolbelt": "^9.6.0",
    "tsd": "^0.29.0",
    "tslib": "^2.6.2",
    "type-fest": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.201",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.9.0",
    "@types/pluralize": "^0.0.33",
    "@types/qs": "^6.9.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsutils": "3.21",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "dev": "concurrently --raw -n node,tsc  \"nodemon -e ts -w ./src -x NODE_ENV=development ts-node --transpileOnly --pretty ./src/server.ts\" \"tsc --noEmit --watch --pretty\"",
    "dev-tsx": "concurrently --raw -n tsx,tsc  \"NODE_ENV=development tsx watch --clear-screen=false ./src/server.ts\" \"tsc --noEmit --watch --pretty\"",
    "start": "NODE_ENV=development ts-node --pretty ./src/server.ts",
    "clean": "rimraf coverage build tmp",
    "build": "swc src -C module.type=commonjs -d build",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "deploy-migrations": "npx prisma migrate deploy"
  },
  "author": "Daniel Yogel",
  "license": "MIT"
}
